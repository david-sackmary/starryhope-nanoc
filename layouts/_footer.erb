  <footer class="row">
    <div class="large-12 columns">
      <hr />
      <div class="row">
        <div class="large-6 columns">
          <p>Copyright &copy; 1998 - <%= Time.now.year %> <%= @config[:author_name] %>.<br>
          <span class="meta">Code available on <a href="https://github.com/jimjimovich/starryhope-nanoc">GitHub</a> | <a href="/credits">Credits</a></span></p>
        </div>
        <div class="large-6 columns">
          <p></p>
        </div>
      </div>
    </div>
  </footer>

  <script>
  document.write('<script src=/assets/javascripts/vendor/' +
  ('__proto__' in {} ? 'zepto' : 'jquery') +
  '.js><\/script>')
  </script>
  <script src="/assets/javascripts/foundation.js"></script>
  <script>
    $(document).foundation();
  </script>

  <script src="/assets/javascripts/app.js"></script>

  <% if @config[:cse_id] %><script src='http://www.google.com/coop/cse/brand?form=cse-search-box&amp;amp;lang=en'></script><% end %>
  
  <% if @config[:ga_enabled] %>
    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', '<%= @config[:ga_id] %>']);
      _gaq.push(['_trackPageview']);
      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>
  <% end %>
  
  <% if @config[:clicky_enabled] %>
    <script src="//static.getclicky.com/js" type="text/javascript"></script>
    <script type="text/javascript">try{ clicky.init(<%= @config[:clicky_id] %>); }catch(e){}</script>
    <noscript><p><img alt="Clicky" width="1" height="1" src="//in.getclicky.com/<%= @config[:clicky_id] %>ns.gif" /></p></noscript>
  <% end %>

  <% if @config[:gplus_enabled] %>
    <script type="text/javascript">
      (function() {
        var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
        po.src = 'https://apis.google.com/js/plusone.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
      })();
    </script>
  <% end %>

  <% if @config[:twitter_enabled] %>  
    <script type="text/javascript">
      (function(){
        var twitterWidgets = document.createElement('script');
        twitterWidgets.type = 'text/javascript';
        twitterWidgets.async = true;
        twitterWidgets.src = 'http://platform.twitter.com/widgets.js';
        document.getElementsByTagName('head')[0].appendChild(twitterWidgets);
      })();
    </script>
  <% end %>